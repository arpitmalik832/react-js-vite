{
  "name": "react-js-vite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite preview",
    "start": "vite --mode development",
    "rm-build": "rimraf build --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "clean": "npm run rm-package-lock && npm run rm-node_modules",
    "build-w-rm": "cross-env NODE_ENV=staging vite build --mode staging",
    "build": "npm run rm-build && npm run build-w-rm",
    "build-w-rm:dev": "cross-env NODE_ENV=development vite build --mode development",
    "build:dev": "npm run rm-build && npm run build-w-rm:dev",
    "build-w-rm:beta": "cross-env NODE_ENV=beta vite build --mode beta",
    "build:beta": "npm run rm-build && npm run build-w-rm:beta",
    "build-w-rm:beta-stg": "cross-env NODE_ENV=beta vite build --mode staging",
    "build:beta-stg": "npm run rm-build && npm run build-w-rm:beta-stg",
    "build-w-rm:prod": "cross-env NODE_ENV=production vite build --mode production",
    "build:prod": "npm run rm-build && npm run build-w-rm:prod",
    "build-w-rm:prod-stg": "cross-env NODE_ENV=production vite build --mode staging",
    "build:prod-stg": "npm run rm-build && npm run build-w-rm:prod-stg",
    "lint-js": "eslint \"{src,public,build_utils,.}/**/*.{js,ts,jsx}\" --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint \"{src,public,build_utils,.}/**/*.{css,scss}\"",
    "check-prettier": "prettier --check **/*.{scss,css,html,js}",
    "lint": "npm run lint-js && npm run lint-css && npm run check-prettier",
    "fix-js": "npm run lint-js -- --fix",
    "fix-css": "npm run lint-css -- --fix",
    "fix-prettier": "prettier --write --ignore-unknown .",
    "fix": "npm run fix-js && npm run fix-css && npm run fix-prettier",
    "lint-staged": "npx lint-staged",
    "grant-permissions:husky": "chmod +x .husky/*",
    "prepare": "husky install &&  npm run grant-permissions:husky",
    "commit": "git-cz",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "test:ss-update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:coverage:pre-commit": "jest --coverage --silent",
    "test:watch:quick": "jest --watch --runInBand",
    "test:coverage:quick": "jest --coverage --maxWorkers=50%",
    "test:coverage:pre-commit:quick": "jest --coverage --maxWorkers=50% --silent",
    "test:clearCache": "jest --clearCache",
    "e2e-open": "cypress open --spec cypress/e2e/app/App.cy.js",
    "e2e-run": "cypress run --spec cypress/e2e/app/App.cy.js",
    "e2e-open-all": "cypress open",
    "e2e-run-all": "cypress run",
    "e2e-open:pre-commit": "npm run e2e-open",
    "e2e-run:pre-commit": "npm run e2e-run",
    "knip": "knip",
    "knip:debug": "knip --debug",
    "knip:check-files": "knip --include files",
    "knip:w/o-devDependencies": "knip --exclude devDependencies",
    "knip-prod": "knip --production",
    "knip-prod:check-files": "knip --production --include files",
    "knip:pre-commit": "npm run knip-prod:check-files",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.6",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "rxjs": "^7.8.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@storybook/addon-essentials": "^8.2.2",
    "@storybook/addon-links": "^8.2.2",
    "@storybook/react-vite": "^8.2.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.13.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.23.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-preset-env": "^9.5.15",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "sass": "^1.77.6",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^5.4.2",
    "stylelint-config-hudochenkov": "^8.0.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "stylelint-order": "^6.0.4",
    "validate-branch-name": "^1.3.0",
    "vite": "^5.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-preload": "^0.4.0",
    "vite-plugin-pwa": "^0.20.0",
    "web-vitals": "^4.2.1"
  }
}
