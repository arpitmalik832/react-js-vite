{
  "name": "react-js-vite",
  "version": "0.0.2",
  "description": "A React JS starter code",
  "sideEffects": false,
  "type": "module",
  "author": "Arpit Malik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arpitmalik832/react-js-vite-starter"
  },
  "engines": {
    "node": ">=20.17.0",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install dependencies')\"",
    "rm-dist": "rimraf dist --glob",
    "rm-build": "rimraf build --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "rm-yarn-lock": "rimraf yarn.lock",
    "clean": "yarn rm-package-lock && yarn rm-node_modules",
    "serve": "cross-env APP_ENV=development vite preview",
    "start": "cross-env APP_ENV=development vite --mode development",
    "build-w-rm": "cross-env APP_ENV=staging vite build --mode staging",
    "build": "yarn rm-dist && yarn build-w-rm",
    "build-w-rm:dev": "cross-env APP_ENV=development vite build --mode development",
    "build:dev": "yarn rm-dist && yarn build-w-rm:dev",
    "build-w-rm:beta": "cross-env APP_ENV=beta vite build --mode beta",
    "build:beta": "yarn rm-dist && yarn build-w-rm:beta",
    "build-w-rm:beta-stg": "cross-env APP_ENV=beta vite build --mode staging",
    "build:beta-stg": "yarn rm-dist && yarn build-w-rm:beta-stg",
    "build-w-rm:prod": "cross-env APP_ENV=production vite build --mode production",
    "build:prod": "yarn rm-dist && yarn build-w-rm:prod",
    "build-w-rm:prod-stg": "cross-env APP_ENV=production vite build --mode staging",
    "build:prod-stg": "yarn rm-dist && yarn build-w-rm:prod-stg",
    "lint-js": "eslint --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint",
    "prettier": "prettier --check",
    "lint": "yarn lint-js \"**/*.{mjs,cjs,js,jsx,mdx}\" && yarn lint-css \"**/*.{css,scss}\" && yarn prettier .",
    "lint-js:fix": "yarn lint-js --fix",
    "lint-css:fix": "yarn lint-css --fix",
    "prettier:fix": "prettier --write --ignore-unknown",
    "lint:fix": "yarn lint-js:fix \"**/*.{mjs,cjs,js,jsx,mdx}\" && yarn lint-css:fix \"**/*.{css,scss}\" && yarn prettier:fix .",
    "lint-staged": "npx lint-staged",
    "grant-permissions:husky": "chmod +x .husky/*",
    "prepare": "husky install &&  yarn grant-permissions:husky",
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ss-update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:clearCache": "jest --clearCache",
    "test:watch:quick": "yarn test:watch --maxWorkers=50%",
    "test:watch:inBand": "yarn test:watch --runInBand",
    "test:coverage:silent": "yarn test:coverage --silent",
    "test:coverage:quick": "yarn test:coverage --maxWorkers=50%",
    "test:coverage:inBand": "yarn test:coverage --runInBand",
    "test:coverage:silent:quick": "yarn test:coverage:silent --maxWorkers=50%",
    "test:coverage:silent:inBand": "yarn test:coverage:silent --runInBand",
    "e2e-open": "cypress open --spec cypress/e2e/app/App.cy.js",
    "e2e-run": "cypress run --spec cypress/e2e/app/App.cy.js",
    "e2e-open-all": "cypress open",
    "e2e-run-all": "cypress run",
    "knip": "knip",
    "knip:debug": "knip --debug",
    "knip:check-files": "yarn knip --include files",
    "knip:w/o-devDependencies": "knip --exclude devDependencies",
    "knip:prod": "knip --production",
    "knip:prod:check-files": "yarn knip:prod --include files",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "node scripts/serve_storybook.js",
    "version-bump:patch": "npx release-it patch",
    "version-bump:minor": "npx release-it minor",
    "version-bump:major": "npx release-it major"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-import-attributes": "^7.25.6",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@release-it/conventional-changelog": "^8.0.2",
    "@storybook/addon-a11y": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-storysource": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/react-webpack5": "^8.3.5",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cypress": "^13.13.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.3.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "express": "^4.21.0",
    "husky": "^9.1.5",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^9.3.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.30.6",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.15",
    "prettier": "^3.3.2",
    "release-it": "^17.7.0",
    "rimraf": "^5.0.7",
    "sass": "^1.77.6",
    "sass-loader": "^16.0.0",
    "storybook": "^8.3.5",
    "style-loader": "^4.0.0",
    "stylelint": "^16.9.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-hudochenkov": "^10.0.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.6.2",
    "url-loader": "^4.1.1",
    "validate-branch-name": "^1.3.0",
    "vite": "^5.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-preload": "^0.4.0",
    "vite-plugin-pwa": "^0.20.0"
  },
  "dependencies": {
    "@arpitmalik832/react-js-rollup-monorepo-library": "^0.3.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  }
}
